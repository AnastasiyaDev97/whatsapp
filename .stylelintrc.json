{
  "extends": ["stylelint-config-standard-scss", "stylelint-config-prettier-scss"],
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["extends"]
      }
    ],
    "selector-class-pattern":null,
    "no-descending-specificity":true,
    "no-irregular-whitespace":true

  }
}
